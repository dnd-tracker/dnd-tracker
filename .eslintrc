{
  "extends": ["@nuxtjs/eslint-config-typescript"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "camelcase": 0,
    "vue/multi-word-component-names" : "off",
    "vue/no-mutating-props" : "off",
    "vue/no-v-html" : "off",
    "no-console": ["error", { 
      "allow": ["info", "warn", "trace", "error"]
      }]
  }
}